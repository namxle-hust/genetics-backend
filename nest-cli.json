{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/samples-handler/src",
  "monorepo": true,
  "root": "apps/samples-handler",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/samples-handler/tsconfig.app.json"
  },
  "projects": {
    "samples-handler": {
      "type": "application",
      "root": "apps/samples-handler",
      "entryFile": "main",
      "sourceRoot": "apps/samples-handler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/samples-handler/tsconfig.app.json"
      }
    },
    "information": {
      "type": "application",
      "root": "apps/information",
      "entryFile": "main",
      "sourceRoot": "apps/information/src",
      "compilerOptions": {
        "tsConfigPath": "apps/information/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "sample-import": {
      "type": "application",
      "root": "apps/sample-import",
      "entryFile": "main",
      "sourceRoot": "apps/sample-import/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sample-import/tsconfig.app.json"
      }
    }
  }
}